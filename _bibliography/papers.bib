@article{do2025precisionpro,
  abbr={TipsRO},
  title={PrecisionPro fusion: Clinical validation of an automated MRI-CT Fusion system for prostate radiotherapy planning},
  author={Do, Deondre and Conlin, Christopher and Baxter, Madison and Christodouleas, John and Dess, Robert and Dragojevic, Irena and Harisinghani, Mukesh and Kamran, Sophia and Moiseenko, Vitali and Nagar, Himanshu and others},
  journal={Technical Innovations & Patient Support in Radiation Oncology},
  pages={100353},
  year={2025},
  publisher={Elsevier},
  html={https://www.sciencedirect.com/science/article/pii/S240563242500054X},
  selected={true},
  pdf={https://www.medrxiv.org/content/10.1101/2025.10.10.25337687v1},
  abstract={We developed and clinically validated PrecisionPro Fusion, an automated pipeline for MRI-CT image registration in prostate radiotherapy. By replacing manual, error-prone alignment processes with a Python/MATLAB-based framework, we reduced processing time to under 15 minutes while maintaining sub-millimeter accuracy. This system facilitates the clinical deployment of focal radiation therapy boosts by ensuring high-precision targeting across heterogeneous hardware.}
}

@article{do2024robustness,
  abbr={medRxiv},
  title={Robustness of a Restriction Spectrum Imaging (RSI) quantitative MRI biomarker for prostate cancer: assessing for systematic bias due to age, race, ethnicity, prostate volume, medication use, or imaging acquisition parameters},
  author={Do, Deondre D and Domingo, Mariluz Rojo and Conlin, Christopher C and Matthews, Ian and Kallis, Karoline and Baxter, Madison T and Ollison, Courtney and Song, Yuze and Xu, George and Zhong, Allison Y and others},
  journal={medRxiv},
  year={2024},
  publisher={Cold Spring Harbor Laboratory Press},
  html={https://www.medrxiv.org/content/10.1101/2024.09.20.24314106v1},
  selected={true},
  abstract={This multi-center study analyzed 1,890 patients across seven institutions to evaluate the robustness of RSIrs as a cancer biomarker. Using multivariate linear mixed-effects models, we demonstrated that the RSIrs metric remains resilient against hardware variations and demographic confounders including age, race, and ethnicity. The findings support RSI as a reliable quantitative tool for large-scale clinical implementation in diverse patient populations.}
}

@article{rojo2025restriction,
  abbr={J. Urology},
  title={Restriction Spectrum Imaging as a quantitative biomarker for prostate cancer with reliable positive predictive value},
  author={Rojo Domingo, Mariluz and Do, Deondre D and Conlin, Christopher C and Bagrodia, Aditya and Barrett, Tristan and Baxter, Madison T and Cooperberg, Matthew and Feng, Felix and Hahn, Michael E and Harisinghani, Mukesh and others},
  journal={The Journal of Urology},
  pages={10--1097},
  year={2025},
  publisher={Wolters Kluwer},
  abstract={This study establishes the positive predictive value of Restriction Spectrum Imaging (RSI) in identifying clinically significant prostate cancer. By comparing RSI-derived metrics against histopathology, we demonstrate that RSI significantly improves the specificity of MRI-based diagnostics, providing a reliable quantitative framework for clinical decision-making.}
}

@article{song2024deep,
  abbr={medRxiv},
  title={Deep learning AI and Restriction Spectrum Imaging for patient-level detection of clinically significant prostate cancer on MRI},
  author={Song, Yuze and Rojo Domingo, Mariluz and Conlin, Christopher C and Do, Deondre D and Baxter, Madison T and Dornisch, Anna and Xu, George and Bagrodia, Aditya and Barrett, Tristan and Harisinghani, Mukesh and others},
  journal={medRxiv},
  year={2024},
  publisher={Cold Spring Harbor Laboratory Press},
  abstract={We introduce a deep learning architecture integrated with RSI signal modeling to enhance patient-level detection of clinically significant prostate cancer. The hybrid AI-RSI approach yields superior sensitivity compared to conventional PI-RADS scoring, highlighting the potential for automated screening tools in radiology workflows.}
}

@article{zhong2024clinical,
  abbr={IJROBP},
  title={Clinical impact of contouring variability for prostate cancer tumor boost},
  author={Zhong, Allison Y and Lui, Asona J and Kuznetsova, Svetlana and Kallis, Karoline and Conlin, Christopher and Do, Deondre D and Domingo, Mariluz Rojo and Manger, Ryan and Hua, Patricia and Karunamuni, Roshan and others},
  journal={International Journal of Radiation Oncology* Biology* Physics},
  volume={120},
  number={4},
  pages={1024--1031},
  year={2024},
  publisher={Elsevier},
  abstract={Quantifying the variability in tumor contouring among radiation oncologists, this paper explores the subsequent impact on dosage delivery in focal boosts. Our results underscore the need for standardized imaging biomarkers to reduce inter-observer variability and ensure consistent treatment planning.}
}

@article{rojo2024utility,
  abbr={Sci Reports},
  title={Utility of quantitative measurement of T2 using restriction spectrum imaging for detection of clinically significant prostate cancer},
  author={Rojo Domingo, Mariluz and Conlin, Christopher C and Karunamuni, Roshan and Ollison, Courtney and Baxter, Madison T courage and Kallis, Karoline and Do, Deondre D and Song, Yuze and Kuperman, Joshua and Shabaik, Ahmed S and others},
  journal={Scientific Reports},
  volume={14},
  number={1},
  pages={31318},
  year={2024},
  publisher={Nature Publishing Group},
  abstract={This research investigates the synergy between RSI and quantitative T2 mapping. By isolating specific signal components, we demonstrate that T2-corrected RSI measurements improve the detection of tumors located in the peripheral and transition zones.}
}

@article{conlin2024improved,
  abbr={medRxiv},
  title={Improved correction of B0 inhomogeneity-induced distortions in diffusion-weighted images of the prostate},
  author={Conlin, Christopher C and Bagrodia, Aditya and Barrett, Tristan and Baxter, Madison T and Do, Deondre D and Hahn, Michael E and Harisinghani, Mukesh G and Javier-DesLoges, Juan F and Kallis, Karoline and Kane, Christopher J and others},
  journal={medRxiv},
  year={2024},
  publisher={Cold Spring Harbor Laboratory Press},
  abstract={Geometric distortions caused by B0 inhomogeneity are a major challenge in prostate DWI. We propose an improved correction algorithm that enhances the spatial fidelity of diffusion maps, ensuring that tumor contours align accurately with structural anatomy.}
}

@article{lui2023reignite,
  abbr={IJROBP},
  title={ReIGNITE radiation therapy boost: a prospective, international study of radiation oncologistsâ€™ accuracy in contouring prostate tumors},
  author={Lui, Asona J and Kallis, Karoline and Zhong, Allison Y and Hussain, Troy S and Conlin, Christopher and Digma, Leonardino A and Phan, Nikki and Mathews, Ian T and Do, Deondre D and Domingo, Mariluz Rojo and others},
  journal={International Journal of Radiation Oncology* Biology* Physics},
  volume={117},
  number={5},
  pages={1145--1152},
  year={2023},
  publisher={Elsevier},
  abstract={The ReIGNITE study evaluates the performance of radiation oncologists in contouring tumors using RSI-augmented MRI versus conventional MRI alone. Results show that RSI provides critical spatial cues that increase the accuracy of focal boost volumes.}
}

@inproceedings{conlin2023background,
  abbr={ISMRM},
  title={Background prostate tissue is quantitatively abnormal on T2-weighted MRI in patients with clinically significant prostate cancer},
  author={Conlin, Christopher C and Karunamuni, Roshan and Hussain, Troy S and Zhong, Allison Y and Kallis, Karoline and Do, Deondre D and Lui, Asona J and Mani, Garnier and Ollison, Courtney and Domingo, Mariluz Rojo and others},
  booktitle={Annual Meeting of ISMRM},
  volume={41},
  year={2023},
  abstract={Our findings suggest that the "normal" background tissue in prostates harboring cancer exhibits distinct quantitative signal changes. This 'field effect' research could lead to new methods for identifying patients at high risk even when visible lesions are ambiguous.}
}
